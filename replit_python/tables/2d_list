#Day 43 Challenge: Bingo List
import random

bingo = []

print("Welcome to Bingo Night!")
print()


def num():
  number = random.randint(1, 90)
  return number


numbers = []
for i in range(8):
  numbers.append(num())
  
numbers.sort()


def prettyPrint():
  for row in bingo:
    print(row)


bingo = [[numbers[0], numbers[1], numbers[2]],
         [numbers[3], "BINGO!", numbers[4]],
         [numbers[5], numbers[6], numbers[7]]]

prettyPrint()
